{
	"redirects": [
		{
			"source": "/directory/arizona/amadio-ranch",
			"destination": "/arizona/amadio-ranch",
			"permanent": true
		},
		{
			"source": "/directory/:state/:slug",
			"destination": "/:state/:slug",
			"permanent": true
		}
	],
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/index.html"
		}
	]
}
